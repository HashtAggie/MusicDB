<p id="notice"><%= notice %></p>

<h1>Listing Songs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Album</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <p><%=link_to song.name, @artist %>
        <td><%=link_to song.album %></td>
        <td><%= song.year %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to'Edit', edit_song_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
